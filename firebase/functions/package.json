{
  "name": "@zm-blood-components/functions",
  "version": "3.2.0",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "clean": "rimraf ./lib && rimraf ./dist",
    "lint": "tslint --project tsconfig.json ",
    "build": "yarn run clean && yarn run lint && yarn run tsc",
    "bundle": "yarn run build && webpack",
    "serve": "yarn run build && firebase emulators:start --only functions",
    "shell": "yarn run build && firebase functions:shell",
    "start": "yarn run shell",
    "logs": "firebase functions:log",
    "test": "jest --silent",
    "style": "yarn run prettier --write . ",
    "deploy-prod": "firelink deploy --only functions -P production ",
    "deploy-dev": "firelink deploy --only functions -P staging "
  },
  "dependencies": {
    "@sendgrid/mail": "^7.4.5",
    "@zm-blood-components/common": "^1.0.0",
    "dayjs": "^1.10.4",
    "firebase-admin": "^9.12.0",
    "firebase-functions": "3.13.2",
    "ics": "^2.31.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "firebase-functions-test": "^0.2.3"
  },
  "fireDependencies": {
    "@zm-blood-components/common": "../../packages/common"
  },
  "fireConfig": {
    "runner": "firebase",
    "outFolderName": ".packages",
    "outFolderLocation": "."
  },
  "private": true
}
