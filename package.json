    "@types/react": "^17.0.1",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.7",
    "@types/uuid": "^8.3.1",
    "@types/yargs": "^16.0.1",
    "cross-env": "^7.0.3",
    "firebase-tools": "^10.2.2",
