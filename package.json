    "react": "^17.0.1",
    "react-div-100vh": "^0.5.6",
    "react-dom": "^17.0.1",
    "react-router-dom": "^6.3.0",
    "react-scripts": "^5.0.1",
    "semantic-ui-react": "^2.0.3",
    "uuid": "^8.3.2",
    "web-vitals": "^1.0.1"
