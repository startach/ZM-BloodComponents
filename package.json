    "jest": "26.6.0",
    "jest-canvas-mock": "^2.3.1",
    "lerna": "^3.22.1",
    "npm-run-all": "^4.1.5",
    "prettier": "2.5.1",
    "sass": "^1.32.6",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "typescript": "^4.1.3",
    "yargs": "^16.2.0"
  },
